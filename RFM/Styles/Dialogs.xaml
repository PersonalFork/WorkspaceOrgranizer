<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dialogs="clr-namespace:RFM.Dialogs">

    <DataTemplate DataType="{x:Type dialogs:ConfirmDialogViewModel}">
        <dialogs:ConfirmDialog />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogs:ItemTypeDialogViewModel}">
        <dialogs:ItemTypeDialog />
    </DataTemplate>

</ResourceDictionary>